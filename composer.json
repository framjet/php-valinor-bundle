{
  "name":              "framjet/valinor-bundle",
  "description":       "A bundle which allows integrating cuyz/valinor library into Symfony application",
  "type":              "symfony-bundle",
  "license":           "MIT",
  "homepage":          "https://github.com/framjet/php-valinor-bundle",
  "keywords":          [
    "valinor",
    "bundle",
    "symfony",
    "framjet"
  ],
  "authors": [
    {
      "name":     "Aurimas Niekis",
      "email":    "aurimas@niekis.lt",
      "homepage": "https://aurimas.niekis.lt/"
    }
  ],
  "require": {
    "php": "~8.0.0 || ~8.1.0",
    "cuyz/valinor": "dev-master",
    "symfony/config": "^4.4|^5.0|^6.0",
    "symfony/dependency-injection": "^4.4|^5.0|^6.0",
    "symfony/http-kernel": "^4.4|^5.0|^6.0"
  },
  "require-dev": {
    "symfony/framework-bundle":               "^4.4|^5.0|^6.0",
    "symfony/browser-kit":                    "^4.4|^5.0|^6.0",
    "framjet/coding-standard":                "^1.0",
    "phpbench/phpbench":                      "^1.2.2",
    "phpunit/phpunit":                        "^9.5.10",
    "psalm/plugin-phpunit":                   "^0.16.1",
    "roave/infection-static-analysis-plugin": "^1.12",
    "squizlabs/php_codesniffer":              "^3.6.2",
    "vimeo/psalm":                            "^4.15.0"
  },
  "autoload": {
    "psr-4": {
      "FramJet\\Packages\\ValinorBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FramJet\\Packages\\ValinorBundleTests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
